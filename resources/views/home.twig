{% extends 'templates/app.twig' %}

{% block content %}
	<table id="users" class="display">
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>Password</th>
			</tr>
		</thead>
		<tbody>
			{% for user in users %}
				<tr>
					<td>{{ user.name }}</td>
					<td>{{ user.email }}</td>
					<td>************</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

{% endblock %}